{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "privacy-analytics-v2": {
      "root": "",
      "sourceRoot": "src-ui",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "targets": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "docs",
            "index": "src-ui/index.html",
            "main": "src-ui/main.ts",
            "polyfills": "src-ui/polyfills.ts",
            "tsConfig": "src-ui/tsconfig.app.json",
            "assets": ["src-ui/favicon.ico", "src-ui/assets"],
            "styles": ["src-ui/styles.scss"],
            "scripts": [
              "src-ui/vendors/palette.js",
              "./node_modules/leader-line/leader-line.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src-ui/environments/environment.ts",
                  "with": "src-ui/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "privacy-analytics-v2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "privacy-analytics-v2:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "privacy-analytics-v2:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src-ui/tsconfig.app.json",
              "src-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "privacy-analytics-v2"
}
